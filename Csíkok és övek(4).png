        <div class="grid ajaxcart__cart-item">
          <div class="grid__item one-third">
            <a href="{{url}}" class="ajaxcart__product-image"><img src="{{img}}" alt="{{name}}"></a>
          </div>
          <div class="grid__item two-thirds ajaxcart__product-meta-wrapper">
